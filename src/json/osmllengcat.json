          },
          {
            "if": {
              "and": [
                "name:ca~"
              ]
            },
            "then": {
              "ca": "https://osm-catalan.github.io/osmllengcat/src/img/icones_senyera/senyera.svg"
            }
          }
        ],
        "condition": {
          "and": []
        }
      },
